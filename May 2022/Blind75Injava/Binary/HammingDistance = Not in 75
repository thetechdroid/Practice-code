class HammingDistance {
    public int hammingDistance(int x, int y) {
        
        int count = 0;
        int num = x ^ y;
        
        while(num != 0){
            
            if(num % 2 == 1)
            count++;
            
            num = num >> 1;
        }

        return count;
        
        
    }
}
